<h1> <%=@tag.name %> </h1>




<form action="/tags/<%= @tag.id %>" method="post" id='newtag'>
<input name="_method" type="hidden" value="patch">
  <label for="tag_name">Name</label>
  <input id="tag_name" name="tag[name]" type="text" value= "<%=@tag.name%>" >
  <button>Submit</button>

<p><a href="/tags/<%= @tag.id %>"
      data-confirm="You sure?"
      data-method="delete"
      rel="nofollow">Delete</a></p>
